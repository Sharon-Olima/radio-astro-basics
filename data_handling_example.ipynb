import numpy as np
import pandas as pd
import matplotlib.pyplot as plt

# Create example dataset
data = pd.DataFrame({
    "x": np.linspace(0, 20, 100),
    "y": np.random.normal(0, 1, 100)
})

data.head()

plt.scatter(data["x"], data["y"])
plt.title("Sample Data Scatter Plot")
plt.xlabel("x")
plt.ylabel("y")
plt.grid(True)
plt.show()

